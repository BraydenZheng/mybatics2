<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatis2.dao.UserMapper">
    <select id="selectByName" parameterType="java.lang.String" resultType="com.example.mybatis2.entity.User">
      select * from mp_user where name = #{name}
    </select>
</mapper>